<%= form_for(@trader, url: admin_trader_index_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
  
    <%= f.label :password %>
    <%= f.password_field :password %>
  
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  
    <%= f.submit "Create Trader" %>
  <% end %>
  
  <%= link_to 'Cancel', admin_dashboard_index_path %>